  "author": "",
  "license": "MIT",
  "dependencies": {
    "@entity-store/core": "workspace:*",
    "@entity-store/types": "workspace:*",
    "@entity-store/pinia-adapter": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^24.3.0",
    "typescript": "^5.9.2"
  }
}
